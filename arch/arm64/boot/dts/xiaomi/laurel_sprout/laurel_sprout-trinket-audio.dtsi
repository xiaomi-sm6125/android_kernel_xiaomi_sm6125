&soc {
    msm_dai_mi2s: qcom,msm-dai-mi2s {
        dai_mi2s1: qcom,msm-dai-q6-mi2s-sec {
            qcom,msm-mi2s-tx-lines = <2>;
        };
    };
    /delete-node/ dai_sen_auxpcm;
    /delete-node/ tdm_sen_rx;

    /delete-node/ cdc_dmic01_gpios;
    /delete-node/ cdc_dmic23_gpios;

    sec_mi2s_gpios: sec_mi2s_pinctrl {
        compatible = "qcom,msm-cdc-pinctrl";
        pinctrl-names = "aud_active", "aud_sleep";
        pinctrl-0 = <&sec_mi2s_gpios_active>;
        pinctrl-1 = <&sec_mi2s_gpios_sleep>;
    };
};

&sm6150_snd {
    status = "okay";
    qcom,model = "trinket-qrd-snd-card";
    qcom,ext-disp-audio-rx = <0>;
    qcom,audio-routing =
        "AMIC1", "MIC BIAS1",
        "MIC BIAS1", "Analog Mic1",
        "AMIC2", "MIC BIAS2",
        "MIC BIAS2", "Analog Mic2",
        "AMIC3", "MIC BIAS3",
        "MIC BIAS3", "Analog Mic3",
        "TX_AIF1 CAP", "VA_MCLK",
        "TX_AIF2 CAP", "VA_MCLK",
        "RX AIF1 PB", "VA_MCLK",
        "RX AIF2 PB", "VA_MCLK",
        "RX AIF3 PB", "VA_MCLK",
        "RX AIF4 PB", "VA_MCLK",
        "HPHL_OUT", "VA_MCLK",
        "HPHR_OUT", "VA_MCLK",
        "AUX_OUT", "VA_MCLK",
        "IN1_HPHL", "HPHL_OUT",
        "IN2_HPHR", "HPHR_OUT",
        "IN3_AUX", "AUX_OUT",
        "TX SWR_ADC0", "ADC1_OUTPUT",
        "TX SWR_ADC2", "ADC2_OUTPUT",
        "RX_TX DEC0_INP", "TX DEC0 MUX",
        "RX_TX DEC1_INP", "TX DEC1 MUX",
        "RX_TX DEC2_INP", "TX DEC2 MUX",
        "RX_TX DEC3_INP", "TX DEC3 MUX";
    qcom,msm-mbhc-gnd-swh = <0>;
    qcom,wsa-max-devs = <0>;

    /delete-property/ qcom,cdc-dmic01-gpios;
    /delete-property/ qcom,cdc-dmic23-gpios;
    /delete-property/ qcom,mbhc-audio-jack-type;

    /delete-property/ qcom,wsa-devs;
    /delete-property/ qcom,wsa-aux-dev-prefix;

    /delete-property/ qcom,mbhc-audio-jack-type;
    fsa4480-i2c-handle;
    qcom,sec-mi2s-gpios = <&sec_mi2s_gpios>;
};


&qupv3_se1_i2c {
    status = "okay";
        spkamp@3a {/* max98927 smartpa device*/
        compatible = "maxim,max98927L";
        status = "okay";
        reg = <0x3a>;
        mono_stereo_mode = <0>;
        interleave_mode = <0>;
        maxim,98927-reset-gpio = <&tlmm 95 0>;
    };
};

