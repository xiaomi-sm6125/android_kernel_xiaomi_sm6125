&reserved_memory {
    pil_camera_mem: camera_region@4ab00000 {
        compatible = "removed-dma-pool";
        no-map;
        reg = <0 0x4ab00000 0 0x500000>;
    };

    pstore: ramoops@61600000 {/*del entry fastboot*/
        compatible = "ramoops";
        reg = <0 0x61600000 0 0x100000>;
        ecc-size        = <0x4>;
        console-size    = <0x40000>;
        record-size = <0x80000>;
    };

    wt_share_mem: wt_region@62000000 {
        compatible = "wt_share_mem";
        no-map;
        reg = <0 0x62000000 0 0x00100000>;
    };
};

&removed_region {
    reg = <0 0x46200000 0 0x4900000>;
};


&pil_adsp_mem {
    reg = <0 0x55300000 0 0x2200000>;
};

&pil_ipa_fw_mem {
    reg = <0 0x57500000 0 0x10000>;
};

&pil_ipa_gsi_mem {
    reg = <0 0x57510000 0 0x5000>;
};

&pil_gpu_mem {
    reg = <0 0x57515000 0 0x2000>;
};

&disp_rdump_memory {
    reg = <0x0 0x5d000000 0x0 0x01000000>;
};
